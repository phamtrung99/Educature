<section class="feature-area">
	<div class="container">
		<div class="row justify-content-center">
			<div class="col-lg-8">
				<div class="section-title text-center">
					<h1>75% Sale OFF Courses</h1>
					<p>
						If you are looking at blank cassettes on the web, you may be very confused at the difference in
						price. You may see some for
						as low as $.17 each.
					</p>
				</div>
			</div>
		</div>

		<div class="feature-inner row">
			{{#each discountcourse}}

			<div class="col-md-4 col-sm-6">
				<div class="box7">
					<i class="ti-crown" style="font-size: 16px;color:deeppink;font-weight:bold">
						{{this.saleoff}}% OFF
					</i>
					<img src="{{this.course_picture_link}}" alt="">
					<div class="box-content">
						<a href="/Courses/{{this.subject_id}}/{{this.course_id}}">
							<h3 class="title">{{this.course_title}}</h3>
						</a>

					</div>
				</div>
			</div>
			{{/each}}
		</div>

	</div>
</section>
